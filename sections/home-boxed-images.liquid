<div data-section-id="{{ section.id }}" data-section-type="home-boxed-images-section" class="home-boxed-images"> 
  <div class="wrapper">
    <div class="grid-uniform">  
      <div class="grid__item extra-large--one-third post-large--one-third large--one-third medium--grid__item small-grid__item"> 
        <div class="hover-effect-wrapper  ">
          <div class="img-hover-effect-1 img-hover-effect last wow fadeIn animated">     
            {% if section.settings.link_5 != blank %}<a class="banner_half_img" href="{{ section.settings.link_5 }}">{% endif%}
            {% if section.settings.image_5 != blank %}
            <img src="{{ section.settings.image_5 | img_url: '1920X' }}" alt="{{ section.settings.title_5 }}" />
            {% else %}
            <img src="//via.placeholder.com/767x1044">
            {% endif%}           
            {% if section.settings.link_5 != blank %}  </a>
            {% endif%}
            <div class="featured-content">       
              {% if section.settings.title_5 != blank %}
              <h6 style="color:{{ section.settings.title_5_color }}">{{ section.settings.title_5 }}</h6>
              {% endif %}
              {% if section.settings.text_5 != blank %}
              <h2 style="color:{{ section.settings.text_5_color }}">{{ section.settings.text_5}}</h2>    
              {% endif %}  
              {% if section.settings.button_5 != blank %}
              <a class="btn five" href="{{ section.settings.link_5 }}" style="color:{{ section.settings.button_5_txt_color }};">{{ section.settings.button_5}}</a>    
              {% endif %}         
            </div>      
          </div>  
        </div>
      </div>
      <div class="grid__item extra-large--two-thirds post-large--two-thirds large--two-thirds medium--grid__item small-grid__item">
        <div class="grid__item">
          <div class="grid__item  home-boxed-images-r1">   
            <div class="hover-effect-wrapper">
              <div class="img-hover-effect-1 img-hover-effect wow fadeIn animated">  
                {% if section.settings.link_1 != blank %}<a class="banner_half_img" href="{{ section.settings.link_1 }}">{% endif%}
                {% if section.settings.image_1 != blank %}
                <img src="{{ section.settings.image_1 | img_url: '1920X' }}" alt="{{ section.settings.title_1 }}" />
                {% else %}
                <img src="//via.placeholder.com/767x740">
                {% endif%}           
                {% if section.settings.link_1 != blank %}  </a>
                {% endif%}
                <div class="featured-content">       
                  {% if section.settings.title_1 != blank %}
                  <h6 style="color:{{ section.settings.title_1_color }}">{{ section.settings.title_1 }}</h6>
                  {% endif %}
                  {% if section.settings.text_1 != blank %}
                  <h2 style="color:{{ section.settings.text_1_color }}">{{ section.settings.text_1}}</h2>    
                  {% endif %}  
                  {% if section.settings.button_1 != blank %}
                  <a class="btn three" href="{{ section.settings.link_1 }}" style="color:{{ section.settings.button_1_txt_color }};">{{ section.settings.button_1}}</a>    
                  {% endif %}         

                </div>      
              </div> 
            </div>

            <div class="grid__item">
              <div class="grid__item  home-boxed-images-r2">

                <div class="hover-effect-wrapper">
                  <div class="img-hover-effect-3 img-hover-effect wow fadeIn animated">
                    {% if section.settings.link_3 != blank %}<a class="banner_half_img" href="{{ section.settings.link_3 }}">{% endif%}
                    {% if section.settings.image_3 != blank %}
                    <img src="{{ section.settings.image_3 | img_url: '1920X' }}" alt="{{ section.settings.title_3 }}" />
                    {% else %}

                    <img src="//via.placeholder.com/767x540">
                    {% endif%}           
                    {% if section.settings.link_3 != blank %}  </a>

                    {% endif%}
                    <div class="featured-content">        
                      {% if section.settings.title_3 != blank %}
                      <h6 style="color:{{ section.settings.title_3_color }}">{{ section.settings.title_3 }}</h6>
                      {% endif %}
                      {% if section.settings.text_3 != blank %}
                      <h2 style="color:{{ section.settings.text_3_color }}">{{ section.settings.text_3}}</h2>    
                      {% endif %}  
                      {% if section.settings.button_3 != blank %}
                      <div class="clear-btn">
                      <a class="btn two" href="{{ section.settings.link_3 }}" style="color:{{ section.settings.button_3_txt_color }};">{{ section.settings.button_3}}</a>    
                      </div>
                      {% endif %}         
                    </div>   
                  </div> 
                </div>

              </div>

            </div>
          </div>  

          <div class="grid__item  home-boxed-images-l1">   
            <div class="hover-effect-wrapper">
              <div class="img-hover-effect-2 img-hover-effect left-side wow fadeIn animated">   
                {% if section.settings.link_4 != blank %}<a class="banner_half_img" href="{{ section.settings.link_4 }}">{% endif%}
                {% if section.settings.image_4 != blank %}
                <img src="{{ section.settings.image_4 | img_url: '1920X' }}" alt="{{ section.settings.title_4 }}" />
                {% else %}
