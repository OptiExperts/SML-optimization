                  {% endif %}  
                  {% if section.settings.button_4 != blank %}
                  <a class="btn one" href="{{ section.settings.link_4 }}" style="color:{{ section.settings.button_4_txt_color }};">{{ section.settings.button_4}}</a>    
                  {% endif %}         
                </div>      
              </div>
            </div>
            <div class="hover-effect-wrapper">
              <div class="img-hover-effect-2 img-hover-effect right-side wow fadeIn animated">   
                {% if section.settings.link_6 != blank %}<a class="banner_half_img" href="{{ section.settings.link_6 }}">{% endif%}
                {% if section.settings.image_6 != blank %}
                <img src="{{ section.settings.image_6 | img_url: '1920X' }}" alt="{{ section.settings.title_6 }}" />
                {% else %}
