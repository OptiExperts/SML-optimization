<div class="container">
  <div class="grid__item">

    {%comment%}
    <div class="navigation-bar-section placement">
      <div class="default-device"> 
        <div class="nav-bar-mobile">
          <nav class="nav-bar" role="navigation">
            <div class="site-nav-dropdown_inner">
              {% include 'site-nav' %}
            </div>
          </nav>
        </div>
      </div>
    </div>  
    {%endcomment%}

    {% include 'new-site-nav' %}

    <div class="Menu-hamburger hidden-dk">
      <i class="fa fa-bars" aria-hidden="true"></i>
    </div>

    <div class="head-logo grid__item logo-center">
      {% include 'logo' %}
    </div>

    <div class="grid__item icon-right ">
      <ul class="menu-icon placement">
        <div class="menu-icon-inner">
          <li class="header-mobile">
            {% comment %}
            <div class="menu-block visible-phone"><!-- start Navigation Mobile  -->
              <div id="showLeftPush">
                <i class="fa fa-bars" aria-hidden="true"></i>

              </div>
            </div><!-- end Navigation Mobile  --> 
            {% endcomment %}
            <div class="Menu-hamburger">
              <i class="fa fa-bars" aria-hidden="true"></i>
            </div>
          </li>
          <li class="navigation-bar-right">

            <div class="slidedown_section">
              <a title="{{ 'layout.customer.log_in' | t }}" id="Togglemodal" class="icon-cart-arrow">{%include 'user-icon'%}</a>
              <div id="slidedown-modal">
                <div class="header-panel-top">
                  <ul>

                    {% if shop.customer_accounts_enabled or section.settings.display_wishlist %}
                    <li>
                      <div class="customer_account">                          
                        <ul>
                          {% if shop.customer_accounts_enabled %}
                          {% if customer %}
                          <li>
                            <a href="/account" title="{{ 'layout.customer.account' | t }}"><i class="fa fa-user" aria-hidden="true"></i> {{ 'layout.customer.account' | t }}</a>
                          </li>
                          <li class="logout-link">
                            <a href="/account/logout" title="{{ 'layout.customer.log_out' | t }}" data-value="value 1"><i class="fa fa-user" aria-hidden="true"></i> {{ 'layout.customer.log_out' | t }}</a>
                          </li>
                          {% else %}
                          <li>
                            <a href="/account/login" title="{{ 'layout.customer.log_in' | t }}"> <i class="fa fa-user-plus" aria-hidden="true"></i> {{ 'layout.customer.log_in' | t }}</a>
                          </li>
                          <li>
                            <a href="/account/register" title="{{ 'layout.customer.create_account' | t }}"> <i class="fa fa-user" aria-hidden="true"></i> {{ 'layout.customer.create_account' | t }}</a>
                          </li>          
                          {% endif %}  
                          {% endif %} 
                          {% if section.settings.display_wishlist %}
                          <li>
                            <a href="/pages/wishlist" title="{{ 'products.wishlist.wishlist_name' | t }}"> <i class="fa fa-heart" aria-hidden="true"></i> {{ 'products.wishlist.wishlist_name' | t }}</a>
                          </li>
                          {% endif %}
                        </ul>
                      </div>     
                    </li>
                    {% endif %}
                  </ul>
                </div>
              </div>
            </div>

          </li>
          <li class="header-mobile logo-mb">
            {% if settings.logo_use_image %}
            <a href="{{shop.url}}" itemprop="url">
              <img class="logo" src="{{ 'logo.png' | asset_img_url: '', crop: 'center'  }}" alt="{{ shop.name }}" itemprop="logo">
            </a>
            {% else %}
            <a href="{{shop.url}}" itemprop="url"  class="shop-name" itemprop="{{ shop.name }}">{{ shop.name }}</a>
            {% endif %}
          </li>
          {% if section.settings.display_search %} 
          <li class="mobile_search"><a href="/search">    {%include 'search-icon'%}</a></li>
          {% endif %} 
          {% if settings.display_currency %} 
          <li class="header_currency">
            <ul class="tbl-list">
              <li class="currency dropdown-parent uppercase currency-block">
                {% include 'currencies-switcher' %}
              </li>
            </ul>
          </li> 
          {% endif %}
          <li class="header-bar__module cart header_cart">
            {% if section.settings.display_minicart %}
            {% include 'mini-cart' %} 
            {% endif %} 
          </li>


        </div>
      </ul> 
      <div class="custom-search hide-mobile">
        {% if section.settings.display_search %}                
        <div id="SearchDrawer" class="ajaxsearch search-bar">
          <div class="search-bar__table search-box">
            <form action="/search" method="get" class="search-bar__table-cell search-bar__form" role="search">
              <input type="hidden" name="type" value="product">  
              <div class="search-bar__table">
                <div class="search-bar__table-cell">
                  <input type="search" id="SearchInput" name="q" value="{{ search.terms | escape }}" placeholder="Search" aria-label="{{ 'general.search.placeholder' | t }}" class="search-bar__input">
                </div>
                <div class="search-bar__table-cell search-bar__icon-cell">
                  <button type="submit" class="search-bar__icon-button search-bar__submit">
                    {%include 'search-icon'%}
                  </button>
                </div>
              </div>
            </form>
          </div>
        </div>
        {% endif %}  
      </div>
    </div>

  </div>
</div>
<style> 
  /* Top block */




  /*{% if template contains 'index'  %} .theme-header .sticky-animate {background: rgb(19,18,16);
  background: linear-gradient(180deg, rgba(19,18,16,1) 3%, rgba(198,145,39,0) 100%);} {% endif %}*/

  .theme-header .header-bar__module.cart .baskettop a.icon-cart-arrow #cartCount { background: {{ section.settings.minicart_count_bg }};color:{{section.settings.minicart_count }};}
  .theme-header .header-bar__module.cart .baskettop a.icon-cart-arrow:hover #cartCount { background: {{ section.settings.minicart_count_hover_bg }};color:{{section.settings.minicart_count_hover_text }};}
  .theme-header .is-sticky .header-sticky {background: {{ section.settings.menubar_bg | color_modify: 'alpha', 0.98 }};}

  /* Logo block */
  .theme-header .theme-logo a,.theme-header .header-mobile #showLeftPush { color:{{section.settings.logo_text }};}
  .theme-header .theme-logo a:hover,.theme-header .header-mobile #showLeftPush:hover { color:{{section.settings.logo_hover_text }};}    
  .theme-header .site-header,.mobile-nav-section,.sticky-animate {background: {{ section.settings.menubar_bg | color_modify: 'alpha', 0.98 }};}  
  .theme-header .menu-tool ul li a,.mobile-nav-section .mobile-nav-trigger {color:{{section.settings.menubar_links}};}  
  .theme-header .menu-tool ul li a:hover {color:{{section.settings.menubar_links_hover}};} 

  .theme-header .menu-tool .site-nav > li > a.current:hover {color:{{section.settings.menubar_links}};} 



  .theme-header .menu-tool .site-nav > li > a.current {border-top:2px solid {{section.settings.menubar_links_hover}};}
  .theme-header .header-bar__module.cart .baskettop{background:{{section.settings.menubar_active_bg}};}
  .theme-header .menu-tool .site-nav >  li > a.current {{section.settings.menu_active}};} 
  .theme-header .site-nav-dropdown,#MobileNav,.mobile-nav__sublist { background: {{ section.settings.menubar_dropdown_bg }};}
  .default-device .site-nav li.mega-menu .site-nav-dropdown , .site-nav li.dropdown ul.site-nav-dropdown {background: {{ section.settings.menubar_dropdown_bg }};}
  .theme-header .site-nav-dropdown .inner > a {color: {{ section.settings.menubar_dropdown_caption }};}    
  .theme-header .site-nav-dropdown .inner > a:hover {color: {{ section.settings.menubar_dropdown_caption_hover }};}    
  .theme-header .site-nav-dropdown .inner .dropdown a,.theme-header .menu-tool .site-nav .site-nav-dropdown li a,.theme-header .site-nav .widget-featured-product .product-title,.theme-header .site-nav .widget-featured-product .widget-title h3,#MobileNav a,.mobile-nav__sublist a,.site-nav .widget-featured-nav .owl-prev a,.site-nav .widget-featured-nav .owl-next a  {color: {{ section.settings.menubar_dropdown_links }};}
  .theme-header .site-nav-dropdown .inner .dropdown a:hover,.theme-header .menu-tool .site-nav .site-nav-dropdown li a:hover,.theme-header .site-nav-dropdown .inner .dropdown a.current,.theme-header .menu-tool .site-nav .site-nav-dropdown li a.current,.theme-header .site-nav .widget-featured-product .product-title:hover,#MobileNav a.current,.mobile-nav__sublist a.current,.site-nav .widget-featured-nav .owl-prev a:hover,.site-nav .widget-featured-nav .owl-next a:hover {color: {{ section.settings.menubar_dropdown_links_hover }};}

  /* Dropdown block */
  .theme-header .navigation-bar-section #Togglemodal i {color: {{ section.settings.dropdown_icon }};}
  .theme-header .navigation-bar-section #Togglemodal i:hover {color: {{ section.settings.dropdown_icon_hover }};}
  .theme-header #slidedown-modal {background: {{ section.settings.dropdown_bg }};}
  .theme-header #slidedown-modal ul li a {color:{{section.settings.dropdown_link}};} 
  .theme-header #slidedown-modal ul li a:hover {color:{{section.settings.dropdown_hover_link}};} 

  /* Search block */     
  .theme-header .search-bar input[type="search"] {color:{{section.settings.search_text }};} 
  .theme-header .header-search span  {color:{{section.settings.search_icon}};} 
  .theme-header .header-search span:hover {color:{{section.settings.search_icon_hover }};} 
  .theme-header .search-bar__form, .theme-header #SearchDrawer,.drawer-page-content:after  {  }


  .theme-header .search-bar__form {background:{{section.settings.search_bg }};}

  .theme-header .search-bar__icon-button span {color:{{section.settings.search_icon}};}

  .theme-header .search-bar__form button,.theme-header .search-bar__icon-button { color:{{section.settings.search_link }};}
  .theme-header .search-bar__form button:hover,.theme-header .search-bar__icon-button:hover { color:{{section.settings.search_link_hover }};}
  .theme-header .header-panel-top ul li.currency-block{ border-bottom:1px solid {{ section.settings.border_between_bars}}; }

  .theme-header .search-bar input[type="search"]::-webkit-input-placeholder  { /* Chrome/Opera/Safari */
    color:{{section.settings.search_text }};
  }
  .theme-header .search-bar input[type="search"]::-moz-placeholder { /* Firefox 19+ */
    color:{{section.settings.search_text }};
  }
  .theme-header .search-bar input[type="search"]:-ms-input-placeholder { /* IE 10+ */
    color:{{section.settings.search_text }};
  }
  .theme-header .search-bar input[type="search"]:-moz-placeholder { /* Firefox 18- */
    color:{{section.settings.search_text }};
  }
  /* Cart Summary block */
  .theme-header a.icon-cart-arrow i  {color: {{ section.settings.minicart_icon }};}
  .theme-header a.icon-cart-arrow:hover i {color: {{ section.settings.minicart_icon_hover }};}
  .theme-header #slidedown-cart .actions, .theme-header  #slidedown-cart  {background: {{ section.settings.minicart_bg }};}
  .theme-header .header-bar__module p {color: {{ section.settings.minicart_text }};}
  .theme-header .header-bar__module a {color:{{section.settings.minicart_link }};}  
  .theme-header .header-bar__module a:hover {color:{{section.settings.minicart_link_hover}};} 
  .theme-header .header-bar__module .btn {color:{{section.settings.minicart_btn_text}};background: {{ section.settings.minicart_btn_bg }};} 
  .theme-header .header-bar__module .btn:hover {color:{{section.settings.minicart_btn_hover_text}};background: {{ section.settings.minicart_btn_hover_bg }};} 
  .theme-header .header-bar__module.cart .wrapper-top-cart {background:{{section.settings.minicart_count}};}

  /* Currency block */

  .theme-header .currency select ,.theme-header .header_currency ul li.currency .selector-arrow::after , .theme-header .mobile_search a{color:{{section.settings.currency_icon}};}   
  .theme-header .currency .selector-arrow:hover select ,.theme-header .header_currency ul li.currency:hover .selector-arrow::after{color:{{section.settings.currency_icon_hover}};}  
  .theme-header .currency:hover:after {border-top-color:{{section.settings.currency_icon_hover}};}
  .theme-header .currency ul li.currency:after {border-top-color:{{section.settings.currency_icon}};}
  .theme-header .currency option {background:{{section.settings.currency_bg}};color:{{section.settings.currency_link}};}


  .theme-header .slidedown_section a.icon-cart-arrow i  {color: {{ section.settings.dropdown_icon }};}
  .theme-header .slidedown_section a.icon-cart-arrow i:hover {color:{{section.settings.dropdown_icon_hover}};}
  .site-nav-dropdown .col-1 ul.dropdown li a, .header-panel-top .customer_account ul li  {border-bottom:1px dashed {{ section.settings.megamenu_border_clr }};}

  .theme-header .site-nav-dropdown .inner .dropdown a::before ,.site-nav li.dropdown ul.site-nav-dropdown a span::before {background: {{section.settings.dropdown_hover_link}}; }
  .theme-header .site-nav {border-top:1px solid {{section.settings.border_between_bars}};}



  .theme-header  li.dropdown>a::before {color:{{section.settings.menubar_links | color_modify: 'alpha', 0.3 }};}
  .theme-header  li.dropdown>a:hover::before {color:{{section.settings.menubar_links }};}


  @media only screen and (min-width:200px) and (max-width:767px) { 


    .theme-header .header-bar__module.cart .baskettop{background:transparent;color:{{section.settings.logo_text }};}

    .theme-header .header-bar__module.cart .baskettop:hover {color:{{section.settings.logo_hover_text }};}

  }

  @media only screen and (min-width:1025px) and (max-width:5000px) { 

    {% if template contains 'index' or template contains 'homepage2' %}  .theme-header .site-header , .mobile-nav-section,.sticky-animate{background: linear-gradient(to top, rgba(255,255,255,0), {{ section.settings.menubar_bg}}) !important;}{% endif %} 

  }

  @media only screen and (min-width:768px) and (max-width:5000px) {  .theme-header li.mobile_search {display:none;} }

  @media only screen and (min-width:968px) {

    #shopify-section-header-01 .Menu-hamburger {
      position: absolute;
      left: 60px;
      top: 50%;
      -webkit-transform: translate(-50%, -50%);
      -moz-transform: translate(-50%, -50%);
      -o-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
      transform: translate(-50%, -50%);
    }

  }

  @media only screen and (max-width:967px) {

    #shopify-section-header-01 .head-logo,
    .Menu-hamburger.hidden-dk {
      display: none!important;
    }

  }

  .head-logo.logo-center {
    margin: 0 auto;
    max-width: 294px;
    float: none;
  }

</style> 


